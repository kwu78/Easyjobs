<!DOCTYPE HTML>

<html>

<head>
    <title></title>
    <meta charset="utf-8" />

    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
    <link rel="stylesheet" type="text/css" href="jobpage.css">

</head>

<body>

    <!-- Banner -->
    <section id="banner">
        <h2>Get a headstart on your next job search</h2>
        <ul class="actions">
            <li><a href="#" class="button special">Get started</a></li>
        </ul>
    </section>

    <!-- One -->
    <section id="one" class="wrapper special">
        <div class="inner">
            <header class="major">
                <h2>Career Fields</h2>
            </header>
            <div class="features">
                <div class="feature">
                    <i class="fa fa-diamond"></i>
                    <h3><a href="/software_engineer">software engineer</a></h3>


                </div>
                <div class="feature">
                    <i class="fa fa-copy"></i>
                    <h3><a href="">Accounting</a></h3>

                </div>
                <div class="feature">
                    <i class="fa fa-paper-plane-o"></i>
                    <h3><a href="/finance">Finance</a></h3>

                </div>
                <div class="feature">
                    <i class="fa fa-save"></i>
                    <h3><a href="">Legal</a></h3>

                </div>
                <div class="feature">
                    <i class="fa fa-envelope-o"></i>
                    <h3><a href="">Management</a></h3>

                </div>
                <div class="feature">
                    <i class="fa fa-envelope-o"></i>
                    <h3><a href="">Marketing</a></h3>

                </div>
            </div>
        </div>
    </section>

    <!-- Two -->
    <section id="two" class="wrapper style2 special">
        <div class="inner narrow">
            <header>
                <h2>Get in touch</h2>
            </header>
            <form class="grid-form" method="post" action="#">
                <div class="form-control">
                    <label for="email">Email</label>
                    <input name="email" id="email" type="email">
                </div>
                <ul class="actions">
                    <li><input value="Send Message" type="submit"></li>
                </ul>
            </form>
        </div>
    </section>



    <!-- Scripts -->
    <script src="assets/js/jquery.min.js"></script>
    <script src="assets/js/skel.min.js"></script>
    <script src="assets/js/util.js"></script>
    <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
    <script src="assets/js/main.js"></script>

